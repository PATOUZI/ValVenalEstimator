@if (Model != null)
{
    <table class="table table-sm table-striped table-bordered m-2">
        <thead>
            <tr>
                <th>Prefecture</th>
                <th>Quartier</th>
                <th>Zone</th>
                <th>Superficie</th>
                <th>Valeur venale du terrain</th>
            </tr>
        </thead>
        <tbody>
            <tr>
                <td>@Model.PrefectureName</td>
                <td>@Model.PlaceName</td>
                <td>@Model.ZoneName</td>
                <td>@Model.Area m2</td>
                <td>@Model.ValVenal</td>
            </tr>
        </tbody>
    </table>
}
